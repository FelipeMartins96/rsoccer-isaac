<robot name="vss">
    <link name="body">
        <inertial>
            <mass value="0.330"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.07 0.07 0.042"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <box size="0.07 0.07 0.042"/>
            </geometry>
        </collision>
    </link>

    <link name="leftWheel">
        <inertial>
            <mass value="0.02"/>
        </inertial>
        <visual>
            <geometry>
                <cylinder radius="0.024" length="0.0025"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.024"/>
            </geometry>
        </collision>
    </link>

    <link name="rightWheel">
        <inertial>
            <mass value="0.02"/>
        </inertial>
        <visual>
            <geometry>
                <cylinder radius="0.024" length="0.0025"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.024"/>
            </geometry>
        </collision>
    </link>

    <joint name="body_leftWheel" type="continuous">
        <origin xyz="0.0 0.03375 0.002" rpy="-1.570796 0.0 0.0" />
        <axis xyz="0.0 0.0 1.0"/>
        <parent link="body" />
        <child link="leftWheel" />
        <limit effort="0.1" velocity="150.0"/>
    </joint>

    <joint name="body_rightWheel" type="continuous">
        <origin xyz="0.0 -0.03375 0.002" rpy="-1.570796 0.0 0.0"/>
        <axis xyz="0.0 0.0 1.0"/>
        <parent link="body" />
        <child link="rightWheel" />
        <limit effort="0.1" velocity="150.0"/>
    </joint>

</robot>